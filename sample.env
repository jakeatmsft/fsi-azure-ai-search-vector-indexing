# Environment variables for fsi-azure-ai-search-vector-indexing
# Copy this file to .env and fill in your own values.

# Azure AI Search (new naming used by most notebooks)
SEARCH_SERVICE_NAME=
SEARCH_ADMIN_KEY=
SEARCH_INDEX_NAME=
SEARCH_INDEX_SCHEMA_FILE=schema.json
# Some notebooks override the API version in code; set if you want to control it globally
SEARCH_API_VERSION=2024-11-01-preview

# Azure OpenAI
OPENAI_API_BASE=https://YOUR-AOAI-SERVICE.openai.azure.com
OPENAI_API_KEY=
OPENAI_API_VERSION=2024-02-01
# Embeddings deployment name (Azure OpenAI deployment ID)
OPENAI_EMBEDDING_MODEL=
# Optional: underlying model name (e.g., text-embedding-3-large)
OPENAI_EMBEDDING_MODEL_NAME=
# Optional: chat/completions model deployment used in 02-process-content.ipynb
OPENAI_GPT_MODEL=

# Azure Blob Storage (source documents)
BLOB_SERVICE_NAME=
BLOB_CONTAINER=
BLOB_KEY=

# Azure Document Intelligence and Cognitive Services
DOC_INTELLIGENCE_ENDPOINT=
DOC_INTELLIGENCE_APIM_KEY=
# Optional: multi-service Cognitive Services key for Search skillsets
COGNITIVE_SERVICES_KEY=

# Local data output directory for processed JSON
DATA_ROOT_DIR=./data

# Agents (05-ai-agent-search.ipynb)
PROJECT_ENDPOINT=
MODEL_DEPLOYMENT_NAME=
# Optional: lower-case variant used by 05-ai-agent-search.ipynb (falls back to "finance-pro")
search_index_name=

# Legacy variable names used by 02-process-content.ipynb
COGNITIVE_SEARCH_SERVICE_NAME=
COGNITIVE_SEARCH_INDEX_NAME=
COGNITIVE_SEARCH_API_KEY=
